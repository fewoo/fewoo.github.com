/*  css reset */
html, body, div, span, iframe, h1, h2, h3, h4, h5, h6, p, blockquote, pre, a, address, big, cite, code, del, em, font, img, ins, small, strong, var, b, u, i, center, dl, dt, dd, ol, ul, li, fieldset, form, label, legend,table,td { margin: 0; padding: 0; }
body {
    font: 12px/20px Arial,Verdana,"Microsoft YaHei","微软雅黑","Lucida Grande","Lucida Sans Unicode",Helvetica,sans-serif;
    color: #000;
    background-color: #F7F0E1;
    -webkit-font-smoothing: antialiased;
}
h1,h2,h3,h4,h5,h6 {font-weight: normal;font-size: 100%;}
ol, ul { list-style: outside none none;}
table{border-collapse: collapse;border-spacing:0;}
:focus {outline: 0 none;}
a:link,a:visited{text-decoration: none;color:#666666;}
a:hover,a:active{text-decoration: underline;color:#666666;}
a img {border: 0 none;}
img {vertical-align: middle;}
table {empty-cells: show;}
ins {text-decoration: none;}
del {text-decoration: line-through;}
em {font-style: normal;}
button::-moz-focus-inner, input[type="reset"]::-moz-focus-inner, input[type="button"]::-moz-focus-inner, input[type="submit"]::-moz-focus-inner, input[type="file"] > input[type="button"]::-moz-focus-inner {
    border: 0 none;
    padding: 0;
}
/* common  */
.clr {
    clear: both;
    display: block;
    font-size: 0;
    height: 0;
    line-height: 0;
    overflow: hidden;
}
.clearfix:after {
    clear: both;
    content: ".";
    display: block;
    height: 0;
    visibility: hidden;
}
.clearfix{  zoom:1; }
.w{width:1200px;margin: 0 auto;}
.w813{width:813px;}
/*  fresh */
.fresh{background-color: #f7f0e1;}
.fresh .hd{height:600px;overflow: hidden;}
.fresh .bd{background:url(../image/fresh/freshbd01.jpg) no-repeat center top;overflow: hidden;zoom:1;}
.fresh .bd .row1{width:1040px;padding:0 80px;margin-top: 240px;}
.shoplist{width:1080px;}
.shoplist li{display:inline;float:left;width:230px;margin-right:40px;margin-bottom: 48px;}
.shoplist .FPic{margin-bottom: 20px;}
.shoplist .FPic a{
    display: inline-block;
    width:230px;
    height:172px;
    -webkit-box-shadow: 0 2px 8px rgba(0,0,0,0.43);
    -moz-box-shadow: 0 2px 8px rgba(0,0,0,0.43);
    box-shadow: 0 2px 8px rgba(0,0,0,0.43);
    background-color:#ffffff;
    filter: progid:DXImageTransform.Microsoft.Shadow(color='#eeeeee',Direction=0, Strength=5)
progid:DXImageTransform.Microsoft.Shadow(color='#eeeeee',Direction=90, Strength=5)
progid:DXImageTransform.Microsoft.Shadow(color='#eeeeee',Direction=180, Strength=5)
progid:DXImageTransform.Microsoft.Shadow(color='#eeeeee',Direction=270, Strength=5);
    overflow:hidden;
}
:root .shoplist .FPic a{filter:none;}
.shoplist .FPic a img{width:230px;height:172px;text-align:center;vertical-align: top;transition: all 0.5s ease;-webkit-transition: all 0.5s ease;  -moz-transition: all 0.5s ease;-o-transition: all 0.5s ease;border:0 none;}
.shoplist .FPic a:hover img{-webkit-transform: scale(1.2);-moz-transform: scale(1.2);-ms-transform: scale(1.2);-o-transform: scale(1.2);transform: scale(1.2);}
.shoplist .FGoodsInfo .FDesc{height:24px;font-size:14px;line-height:24px;text-align:center;color:#690a04;overflow: hidden;}
.shoplist .FGoodsInfo  .FDesc a{color:#690a04;cursor: pointer;}
.shoplist .FGoodsInfo  .FDesc a:link,.fresh .FGoodsInfo .FDesc a:visited{color:#690a04;}
.shoplist .FGoodsInfo  .FDesc a:hover,.fresh .FGoodsInfo .FDesc a:active{color:#f00;}
.shoplist .FGoodsInfo  .FPrice{height:18px;line-height: 18px;font-size:12px;color:#690a04;text-align:center;overflow: hidden;}
.shoplist .FGoodsInfo .FPrice .FRmb{margin-right:5px;}

.fresh .bd .row2{
    padding-top: 204px;
    height:226px;
    background: url("../image/fresh/freshbd02.jpg") no-repeat;
    margin-bottom: 13px;
}
.fresh .row2 .cnt{width:813px;margin:0 auto;}
.fresh .row2 .cnt .preser-intro{border:1px solid #74716b;}
.fresh .row2 .cnt .preser-intro li{float:left;width:365px;padding:17px 20px;text-align: center;}
.fresh .row2 .cnt .preser-intro .first{background: url("../image/fresh/divide.gif") no-repeat right center;}
.fresh .row2 .cnt .preser-intro .tit{display:block;margin-bottom:11px;font:500 20px/2 simHei,"黑体";color:#6a0c02;}
.fresh .row2 .cnt .preser-intro li p{margin-bottom:20px;font:500 15px/24px simHei,"黑体";color:#403d38;}
.fresh .row3{height:506px;margin-bottom:137px;padding-top:1px;position: relative;background:url(../image/fresh/freshbd03.jpg) no-repeat;}
.fresh .row3 .tit h1{display: none;}
.fresh .row3 .tit .declare{position:absolute;left:50%;top:168px;margin-left:-96px;font:500 22px/26px simHei, "黑体";color:#6a0c02;}
.fresh .row3 .declareTable  .icon{display: block;width:50px;height:50px;background-image: url("../image/fresh/icon/fresh-icon1.gif");background-repeat: no-repeat;}
.fresh .row3 .declareTable{table-layout: fixed;width:916px;margin-left:-458px;position: absolute;top:252px;left:50%;}
.fresh .row3 .declareTable td{padding:20px 5px 20px;border-width: 1px 0 1px 1px;border-color:#000;border-style: solid;vertical-align: top;}
.fresh .row3 .declareTable td.last{border-width: 1px;}
.fresh .row3 .declareTable td p{margin-bottom:24px;font:500 15px/19px simHei,"黑体";color:#6a0c02;}
.fresh .row3 .declareTable td.first p{padding-left:25px;}
.fresh .row3 .declareTable td.second p{padding-left:53px;}
.fresh .row3 .declareTable td.last p{padding-left:45px;}
.fresh .row3 .declareTable h2{padding:17px 0 0 87px;margin-bottom:27px;font-size:0;line-height: 0;overflow: hidden;text-indent: -9999px;}
.fresh .row3 .declareTable .icon-1{background-position: 0 0;}
.fresh .row3 .declareTable .icon-2{background-position: -50px 0 ;}
.fresh .row3 .declareTable .icon-3{background-position: -100px 0 ;}
