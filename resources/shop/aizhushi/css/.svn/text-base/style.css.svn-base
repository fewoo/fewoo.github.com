}
.step{
    width:24px;
    height:24px;
    -moz-border-radius: 12px;
    -webkit-border-radius: 12px;
    border-radius: 12px;
    background-color:#dbd9d7;
    position: absolute;
    top:50%;
    margin-left:-12px;
    margin-top:-12px;
    text-align: center;
    line-height: 20px;
}
.step01{left:20%;  }
.step02{  left:40%; }
.step03{left:60%;}
.step04{left:80%;}
.stepOn{
    width:18px;
    height:18px;
    background-color:#40b117;
    border:1px solid #31920d;
    -moz-border-radius: 10px;
    -webkit-border-radius: 10px;
    border-radius: 10px;
    position: absolute;
    left:50%;
    top:50%;
    margin-left:-10px;
    margin-top:-10px;
    font:16px/18px 'Microsoft YaHei';
    color:#fff;
}
.stepOff{
    width:18px;
    height:18px;
    border:1px solid #DBD9D7;
    -moz-border-radius: 10px;
    -webkit-border-radius: 10px;
    border-radius: 10px;
    position: absolute;
    left:50%;
    top:50%;
    margin-left:-10px;
    margin-top:-10px;
    font:16px/18px 'Microsoft YaHei';
    color:#000;
}
.stepText{
    width:200px;
    position: absolute;
    top:30px;
    font:15px/23px 'Microsoft YaHei';
    color:#5b5b5b;
    overflow: hidden;
    text-align: center;
    margin-left:-100px;
}
.stepText01{left:20%;}
.stepText02{left:40%;}
.stepText03{left:60%;}
.stepText04{left:80%;}
