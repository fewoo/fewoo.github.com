*{word-wrap: break-word;word-break: break-all;}
body,div,ol,ul,h1,h2,h3,h4,h5,h6,p,th,td,dl,dd,form,iframe,input,textarea,select,label,article,aside,footer,header,menu,nav,section,time,audio,video {
    margin:0;
    padding:0;
}
article,aside,footer,header,hgroup,nav,section,audio,canvas,video,img {
    display: block;
}
audio,canvas,video {
    display: inline-block;
}
audio:not([controls]) {
    display: none;
    height: 0;
}
[hidden],
template {display: none;}
html,body {
    width:100%;
    height:100%;
    font-size: 100%;
    font-family:Arial,"Helvetica Neue",STHeiti,"Droid Sans Fallback";
    -webkit-text-size-adjust: 100%;
    -ms-text-size-adjust: 100%;
    outline: 0 none;
    -webkit-tap-highlight-color: rgba(255,255,255,0);
    color:#333;
}
h1,h2,h3,h4,h5,h6{font-family:Arial,"Helvetica Neue",STHeiti,"Droid Sans Fallback";font-size: 100%;font-weight: normal;}
textarea {resize: none;}
iframe,img {border:0; }
ul,ol {list-style:none;}
button, input, select, textarea {font-family: inherit; font-size: 100%; margin: 0;/*-webkit-user-modify:read-write-plaintext-only;*/}
button,input {-webkit-appearance: none;}
input{-webkit-tap-highlight-color: rgba(0,0,0,0);-webkit-tap-highlight-color: transparent;}
input::-ms-clear {display: none;}
::-webkit-input-placeholder{color:rgba(153, 153, 153,0.5);}
:-moz-placeholder{color:rgba(153, 153, 153,0.5);}
a,button,input {outline: 0 none;text-decoration: none}
a{color:#333;background: transparent;}
a:visited,a:hover,a:active{color:#333;}
b, strong { font-weight: bold;}
em{font-style: normal;}
hr {-moz-box-sizing: content-box; box-sizing: content-box; height: 0;}
mark {background: #ff0; color: #000; }
pre {white-space: pre-wrap;}
button, select {text-transform: none;}
button[disabled],html input[disabled] {cursor: default;}
input[type="checkbox"],
input[type="radio"] {box-sizing: border-box; padding: 0;}
input[type="search"]::-webkit-search-cancel-button,
input[type="search"]::-webkit-search-decoration {-webkit-appearance: none;}
button::-moz-focus-inner,
input::-moz-focus-inner {border: 0; padding: 0;}
textarea {overflow: auto; vertical-align: top;}
table {border-collapse: collapse;border-spacing: 0;}
.fr{display:inline;float:right;}
.fl{display:inline;float:left;}
.clear:after {content: "."; display: block; clear: both; height: 0; overflow: hidden; visibility: hidden}
.clear {zoom: 1;}
body,.global{min-width: 320px!important;background:#f4f4f4;}
.layout{min-width: 300px;max-width: 640px;margin: 0 auto;}
.main{min-width: 320px;/*min-width: 320px;max-width: 640px;margin:0 auto;overflow: hidden;*/}
.overhd{overflow: hidden;}
.hide{display: none!important;}
.show{display: block!important;}
.box{-webkit-box-sizing: border-box;-moz-box-sizing: border-box;box-sizing: border-box;}

