#wrapper {position:absolute; z-index:1; top:48px; bottom:48px; left:0; width:100%;  overflow:hidden;}
#scroller {position: absolute; z-index: 1; width: 100%;  -webkit-transform: translateZ(0);-moz-transform: translateZ(0); transform: translateZ(0);
}
.entrance{padding:11px 10px 10px;}
.reg.main{min-width: 300px;}
.entrance .item{width: 100%;}
.entrance.reg .item{margin-bottom: 10px;}
.entrance .item .tx-input{width: 100%;padding:16px 10px;margin-top:-1px;line-height: 16px; border: 1px solid #e5e5e5;background: #fff; font-size: 14px;-webkit-box-sizing: border-box; -moz-box-sizing: border-box; box-sizing: border-box;-webkit-border-radius: 0;-moz-border-radius: 0;border-radius: 0;}
.entrance .item .t-input-focus{border-color:#999;}
.entrance .item .txt-sms-captcha{width:58.82353%;}
.entrance .item .txt-invite{width:58.82353%;}
.entrance  .txt-invite{display: none;}
.entrance .item-intro a{display:block;text-decoration: underline;color: #ff9400;line-height: 16px;font-size: 13px;}
.entrance .item-tips{/*margin-bottom: 10px;*/}
.entrance .item .err-msg {font-size: 13px;line-height: 1; color: #f25454;}
.entrance .item-login-option,.entrance .item-login-other{padding:10px 0;margin-bottom: 9px;}
.entrance .item-login-option .retrieve-password{display:inline;float:right;width: 50%;text-align: right;line-height: 16px;font-size:14px;}
.retrieve-password a,.retrieve-password a:visited,.retrieve-password a:hover,.retrieve-password a:active{color:#999;}
/*.entrance .item-password-repeat .tx-input{border-top-color:#fff!important;}*/
.item-img-captcha .extra-tips,.item-invite .extra-tips  {margin: 8px 0 22px;line-height:16px;font-size: 13px; color: #878787;}
.item-img-captcha .extra-tips a:link,.item-sms-captcha .extra-tips a:visited,.item-sms-captcha .extra-tips a:active,.item-invite .extra-tips a:link,.item-invite .extra-tips a:visited{color:#71a91b;}
.item-img-captcha{margin-bottom: 25px;}
.entrance .item .txt-img-captcha{width:67.647059%;}
.entrance .item-img-captcha-xl .txt-img-captcha{width: 58.82353%;}
.entrance .item-img-captcha-xl .captcha-img{width: 38.235294%!important;}
.entrance .item .captcha-img{display:inline;float:right;width:29.411744%;margin:7px 0 8px;height:35px;text-align:center;line-height: 35px;-webkit-border-radius:4px;-moz-border-radius:4px;border-radius: 4px;}
.entrance .item .captcha-img img{max-width:100%;}
.entrance .item-btns{margin-bottom: 9px;}
.entrance .item-btns .btn{width: 100%;height:40px;margin:0;line-height: 40px;}
.entrance .item-btns .btn-disabled{color: #bdbdbd;background: #e2e2e2;border: none;cursor: default;}
.entrance .item-login-other{padding:5px 0;}
.entrance .item-login-other .register-free{display:inline;float:left;width:100%;text-align:center;line-height: 16px;color:#ff9400;}
.register-free a,.register-free a:visited,.register-free a:hover,.register-free a:active{color:#ff9400;}

.main .item-sms-captcha .btn-retransmit{display: inline;float:right;width:38.235294%;height:50px;font-size:12px;color:#ff9400;text-align:center;line-height:50px;border:1px solid #ff9400;background:#fff;cursor:pointer;-webkit-box-sizing:border-box;-moz-box-sizing:border-box;box-sizing: border-box;overflow: hidden;-webkit-user-select: none;-moz-user-select: none;user-select: none;}

.main .item-sms-captcha .btn-retransmit:active,.main .item-sms-captcha .btn-retransmit-active{color:#fff;background: #ff9400;}
.main .item-sms-captcha  .btn-retransmit-disabled,.main .item-sms-captcha  .btn-retransmit-disabled:active{color: #bdbdbd;background: #e2e2e2;border-color:#e2e2e2;}
.retripwd .item-sms-captcha{position: relative;}
.retripwd .item-sms-captcha .btn-retransmit{position: absolute;top:7px;right:8px;width:100px;height:35px;line-height: 35px;}
.agreement{height:100%;background: #fff;}
.agreement-main{padding: 10px;}
.agreement-main p{text-indent:2em;font-size:14px;color:#999;line-height: 24px;}